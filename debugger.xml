        </Renderer>
      </Renderer>
      <Renderer ID="CompoundTypeRenderer">
        <option name="NAME" value="byte[] as hexa rows" />
        <option name="QUALIFIED_NAME" value="byte[]" />
        <Renderer ID="LabelRenderer" role="label">
          <option name="QUALIFIED_NAME" value="byte[]" />
          <option name="ON_DEMAND" value="false" />
          <option name="LABEL_EXPRESSION" expression="StringBuilder builder = new StringBuilder();&#10;boolean first = true;&#10;for (byte b : this) {&#10;    if (!first) builder.append(&quot;\n&quot;);&#10;    builder.append(&quot;0x&quot;);&#10;builder.append(String.format(&quot;%x&quot;, b).toUpperCase());&#10;    first = false;&#10;}&#10;return builder.toString();" language="JAVA" mode="CODE_FRAGMENT" />
        </Renderer>
      </Renderer>
    </CustomRenderers>
